

@model IEnumerable<TripNPick.Models.FilteredFarmViewModel>
<table>
    <tr>
        <td>Farm Name</td>
        <td>Suburb Name</td>
        <td>State Name</td>
        <td>Latitude</td>
        <td>Longitude</td>
        <td>Suburb_lat</td>
        <td>Suburb_lng</td>
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.farmName</td>
        <td>@item.suburbName</td>
        <td>@item.stateName</td>
        <td>@item.farm_lat</td>
        <td>@item.farm_lng</td>
        <td>@item.suburb_lat</td>
        <td>@item.suburb_lng</td>
    </tr>
    }

</table>

