@model TripNPick.Models.DictionaryView
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>doTheDew</title>
</head>
<body>
    <div> 
        @foreach (var item in Model.farmDictionary) {
             <ul>
                 <li>@item.Key.farmName
                 <ul>
                     @foreach (var subitem in item.Value) {
                         <li>@subitem.attractionName       <b>Distance</b> : @subitem.distance km</li>
                     }
                 </ul>
                 </li>
             </ul>
        }
    </div>
</body>
</html>
